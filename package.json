{
  "name": "simple-react",
  "version": "1.0.0",
  "description": "simple test",
  "scripts": {
    "start": "node server.js",
    "watch": "npm run watch:js && nodemon --watch src",
    "watch:js": "browserify src/index.js -o static/bundle.js -t [ babelify --presets [ es2015 react ] ]",
    "build": "babelify",
    "lint": "eslint src"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/MBing/simpletest.git"
  },
  "keywords": [
    "react",
    "reactjs",
    "boilerplate",
    "hot",
    "reload",
    "hmr",
    "live",
    "edit",
    "webpack"
  ],
  "author": "Martin Bing",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/MBing/simpletest.git/issues"
  },
  "browserify": {
    "transform": [
      [
        "babelify",
        {
          "presets": [
            "es2015",
            "react"
          ]
        }
      ]
    ]
  },
  "devDependencies": {
    "babel-core": "^6.0.20",
    "babel-eslint": "^4.1.3",
    "babel-loader": "^6.0.1",
    "babel-preset-es2015": "^6.0.15",
    "babel-preset-react": "^6.0.15",
    "babel-preset-stage-0": "^6.0.15",
    "babelify": "^7.3.0",
    "browserify": "^13.1.0",
    "eslint": "^1.10.3",
    "eslint-plugin-react": "^3.6.2",
    "nodemon": "^1.10.0",
    "react-hot-loader": "^1.3.0",
    "webpack": "^1.12.2",
    "webpack-dev-server": "^1.12.1"
  },
  "dependencies": {
    "react": "^0.14.6",
    "react-bootstrap": "^0.30.1",
    "react-dom": "^0.14.6",
    "react-redux": "^4.4.5",
    "react-router": "^2.6.0",
    "react-router-bootstrap": "^0.23.1",
    "redux": "^3.5.2"
  }
}
